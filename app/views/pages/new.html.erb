<div class="new-note">
  <div class="new-note-container">
    <form class="form">
      <fieldset>
        <label for="title" ng-model="note.title">Title</label>
        <input name="title" />
      </fieldset>

      <fieldset>
        <label for="description">Description</label>
        <textarea name="description" ng-model="note.description"></textarea>
      </fieldset>

      <fieldset>
        <label for="content" ng-model="note.content">Content</label>
        <textarea name="content"></textarea>
      </fieldset>

      <button class="btn" ng-click="newMoviesCtrl.saveMovie(Movie)">Save</button>
    </form>
  </div>
</div>
